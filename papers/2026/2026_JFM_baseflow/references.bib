			Keywords = {coherent turbulence; effective diffusion coefficient; hyporheic exchange; streams; turbulence; 1805 Hydrology: Computational hydrology; 1813 Hydrology: Eco-hydrology; 1816 Hydrology: Estimation and forecasting (4315); 1829 Hydrology: Groundwater hydrology},
			Month = {05},
			Number = {5},
			Pages = {W05548},
			Publisher = {AGU},
