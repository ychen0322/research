	Journal = {Shock Waves},
	Month = {Jan},
	Number = {1},
	Pages = {73--99},
	Title = {{An adaptive ALE scheme for non-ideal compressible fluid dynamics over dynamic unstructured meshes}},
	Url = {https://doi.org/10.1007/s00193-018-0840-2},
	Volume = {29},
	Year = {2019},
	Bdsk-Url-1 = {https://doi.org/10.1007/s00193-018-0840-2}}

@article{Temkin_JSV_1977,
	Abstract = {Thunderclap generation by lightning is analyzed in terms of a linear model in which heat addition-rather than an initial distribution of condensation---is responsible for the emission. The analysis is based on the basic formulation proposed by Chu to study the fundamental properties of sound waves emitted by heat sources. For the lightning discharge it is assumed that heat is released along a straight line of infinite extent. The time variations of heat release are taken to be of the simplest type to allow closed form solutions. The analysis results in acoustic signatures which are very similar to those obtained in the field. They indicate that some of the main features in the observed thunderclap signatures are due to the time duration of the heat-addition stages. The results are also used to compute the far field power spectrum of a thunderclap. The calculated spectrum is found to be similar to those obtained experimentally. In particular, its maximum amplitude peak occurs at a frequency which is of the same order of magnitude as the values reported in the literature.},
	Author = {S. Temkin},
	Date-Added = {2019-01-31 23:12:08 +0100},
	Date-Modified = {2019-08-30 22:01:08 +0200},
	Issn = {0022-460X},
	Journal = {Journal of Sound and Vibration},
	Number = {3},
	Pages = {401 - 414},
	Title = {A model for thunder based on heat addition},
	Url = {https://doi.org/10.1016/0022-460X(77)90567-3},
