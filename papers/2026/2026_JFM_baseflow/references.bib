			Journal = {Experiments in Fluids},
			Number = {5},
			Pages = {1089--1106},
			Title = {Instantaneous planar pressure determination from PIV in turbulent flow},
			Url = {http://dx.doi.org/10.1007/s00348-011-1237-5},
			Volume = {52},
			Year = {2012},
