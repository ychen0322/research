	Journal = {International Journal of Aeroacoustics},
	Number = {1},
	Pages = {11-19},
	Title = {The Acoustic Spectrum in the Sound Field of Isotropic Turbulence},
	Url = {http://dx.doi.org/10.1260/1475472053730011},
	Volume = {4},
	Year = {2005},
	Bdsk-Url-1 = {http://dx.doi.org/10.1260/1475472053730011}}

